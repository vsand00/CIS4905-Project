<div class="container">
  <div class="row">
    <div class="col-4">
    </div>
    <div class="col">
      <h1>Login</h1>
         <%= form_tag '/login' do %>
         <div class="row">
         <%= label_tag :username%>
         <%= text_field_tag :username %>
         </div>
         <div class="row">
         <%= label_tag :password%>
         <%= password_field_tag :password%>
         </div>
         <%= submit_tag "Login"%>
      <%end%>
       
      <%= button_to "Back to Welcome", '/welcome', method: :get %>
   </div>
   <div class="col-4">
   </div>
  </div>
</div>




  